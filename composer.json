{
    "name": "recca0120/cart",
    "description": "Cart",
    "keywords": [],
    "license": "MIT",
    "authors": [
        {
            "name": "recca0120",
            "email": "recca0120@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "symfony/http-foundation": "^2.8|^3.0",
        "illuminate/support": "^5.1"
    },
    "require-dev": {
        "mockery/mockery": "~0.9.4",
        "phpunit/phpunit": "~4.8|~5.4|~6.1",
        "nesbot/carbon": "~1.20"
    },
    "autoload": {
        "psr-4": {
            "Recca0120\\Cart\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Recca0120\\Cart\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Recca0120\\Cart\\CartServiceProvider"
            ]
        }
    }
}
